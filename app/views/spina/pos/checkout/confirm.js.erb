<% if @order.payment_method == 'cash' %>
  var printer = new Spina.PosPrinter("<%= pos_preferences.printer_ip %>", "<%= current_user.name %>");
  printer.openDrawerInstantly();
<% end %>

webkit.messageHandlers.spina.postMessage("finished");
Turbolinks.visit("<%= finished_checkout_index_path(change: @change, order_id: @order.id) %>");
